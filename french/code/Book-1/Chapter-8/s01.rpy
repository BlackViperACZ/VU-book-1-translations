# game/code/Book-1/Chapter-8/s01.rpy:70
translate french b1c08s01_eca0cc9b:

    # mc "*Smirks* Right, right."
    mc "*Sourit* Oui, oui."

# game/code/Book-1/Chapter-8/s01.rpy:74
