translate turkish b1c06cs07_end_2bb131e3:

    # mc "No, I'm good Sally. Thank you, though."
    mc "Hayır, ben iyiyim Sally. Yine de teşekkür ederim."

# game/code/Book-1/Chapter-6c/s07.rpy:651
translate turkish b1c06cs07_end_31695b5f:

